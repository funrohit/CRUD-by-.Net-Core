@model List<mycore.Models.EmpModel>


  <table>
  <tr>

      <th>ID</th>
      <th>Email</th>
      <th>Name</th>
      <th>Delete</th>
      <th>Edit</th>

  </tr>    
  @foreach(var item in Model)
    {
        <tr>

            <td>@item.Id</td>
            <td>@item.Email</td>
            <td>@item.Name</td>
            <td><a href="@Url.Action("del","home")">delete</a></td>
              <td><a href="@Url.Action("editing","home")">edit</a></td>
        </tr>
    }

  
  
  </table>

